{
  "kind": "Deployment",
  "apiVersion": "v1",
  "metadata": {
    "name": "capstone",
    "labels": {
      "app": "capstone"
    }
  },
  "spec": {
    "replicas": 1,
    "selector": {
        "matchLabels":{
            "app": "capstone"
        }
    }
    "template": {
      "metadata": {
        "labels": {
          "app": "capstone"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "bluecapstone",
            "image": "2002714/capstone",
            "imagePullPolicy": "Always",
            "ports": [
              {
                "containerPort": 80
              }
            ]
          }
        ]
      }
    }
  }
}